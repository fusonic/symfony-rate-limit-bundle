{
    "name": "fusonic/rate-limit-bundle",
    "description": "Simple rate limiting based on routes.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Zangerle",
            "email": "michael.zangerle@fusonic.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fusonic\\RateLimitBundle\\": "Fusonic/RateLimitBundle/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fusonic\\RateLimitBundle\\Tests\\": "Fusonic/RateLimitBundle/tests"
        }
    },
    "require": {
        "php": "^7.2.5",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/monolog-bridge": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/monolog-bundle": "^3.5",
        "doctrine/cache": "^1.8",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.11",
        "symfony/yaml": "^4.4",
        "symfony/browser-kit": "^4.4"
    },
    "scripts": {
        "phpstan": "php vendor/bin/phpstan analyse Fusonic/RateLimitBundle/src --level=6 -c phpstan.neon"
    }
}
